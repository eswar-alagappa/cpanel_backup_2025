}

#nav.affix {
    position: fixed;
    top: 0;
    width: 100%;
    z-index:10;
	box-shadow: 0 2px 2px 0 rgba(0,0,0,.14), 0 3px 1px -2px rgba(0,0,0,.2), 0 1px 5px 0 rgba(0,0,0,.12);
}
#nav.affix .navbar-default{
	border:none;
}
#sidebar.affix-top {
    position: static;
}

#sidebar.affix {
    position: fixed;
    top: 80px;
}
.right-info:before{
	content: "";
    height: 48px;
    position: absolute;
    right: 0;
    top: 14px;
	width: 35%;
	background: #f36d21;
	z-index:1;
}
.right-info:after{
	border-color:rgba(0, 0, 0, 0) #f36d21 #f36d21 rgba(0, 0, 0, 0);
	-moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    -o-border-image: none;
    border-image: none;
    border-style: solid;
    border-width: 24px 15px;
    content: "";
    height: 5px;
    position: absolute;
    right: 35%;
    top: 14px;
    width: 0;
	z-index:1;
}
.left-info{
	float:left;
	width:50%;
}
.left-info>p{
	color: #fff;
    margin: 0px;
    font-weight: 300;
    letter-spacing: 0.7px;
}
.right-info{
	width:50%;
	float:left;
}
.right-info>ul{
	list-style: none;
    z-index: 26;
    position: absolute;
    top: 21px;
    width: 50%;
    right: 0%;
    text-align: center;
    padding-left: 100px;
    margin: 0px;
}
.right-info>ul>a>li{
	display:inline-block;
	margin:0px 15px;
	color:#fff;
	width:35px;
	height:35px;
	border-radius:50%;
	border:2px solid #fff;
	text-align:center;
	line-height:31px;
}
.right-info>ul>li{
	display:inline-block;
}
.right-info>ul>li>a{
	color:#fff;
}
.right-info>ul>a>li:hover{
	background-color:#145dab;
	border:2px solid #145dab;
	box-shadow:0px 0px 13px 4px #b9b9b9;
	transition:0.4s ease-in-out;
	color:#fff;
	-o-transition:0.4s ease-in-out;
	-webkit-transition:0.4s ease-in-out;
	-ms-transition:0.4s ease-in-out;
	-moz-transition:0.4s ease-in-out;
}
