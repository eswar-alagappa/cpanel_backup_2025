 <script type='text/javascript'>
     $(function () {
         var overlay = $('<div id="overlay"></div>');
         
         $('.x').click(function () {
             $('.popup').hide();
             $('.popup1').hide();
             $('.popup2').hide();
             $('.popup3').hide();
             $('.popup4').hide();
             $('.popup5').hide();
             $('.popup6').hide();
             overlay.appendTo(document.body).remove();
             return false;
         });

         $('.click').click(function () {
             overlay.show();
             overlay.appendTo(document.body);
             $('.popup').show();
             return false;
         });
         $('.click1').click(function () {
             overlay.show();
             overlay.appendTo(document.body);
             $('.popup1').show();
             return false;
         });
         $('.click2').click(function () {
             overlay.show();
             overlay.appendTo(document.body);
             $('.popup2').show();
             return false;
         });
         $('.click3').click(function () {
             overlay.show();
             overlay.appendTo(document.body);
             $('.popup3').show();
             return false;
         });
         $('.click4').click(function () {
             overlay.show();
             overlay.appendTo(document.body);
             $('.popup4').show();
             return false;
         });
         $('.click5').click(function () {
             overlay.show();
             overlay.appendTo(document.body);
             $('.popup5').show();
             return false;
         });
         $('.click6').click(function () {
             overlay.show();
             overlay.appendTo(document.body);
             $('.popup6').show();
             return false;
         });
         $('.click6').click(function () {
             overlay.show();
             overlay.appendTo(document.body);
             $('.popup6').show();
             return false;
         });
     });
</script>
